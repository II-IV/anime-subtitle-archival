Dialogue: 0,0:22:28.92,0:22:29.80,ls_ed_eng,,0000,0000,0000,,ZARADO!
Dialogue: 0,0:22:30.55,0:22:31.47,ls_ed_eng,,0000,0000,0000,,ERADO!
Dialogue: 0,0:22:32.17,0:22:33.43,ls_ed_eng,,0000,0000,0000,,GARADO!
Dialogue: 0,0:22:37.85,0:22:39.85,ls_ed_eng,,0000,0000,0000,,SHINE, JANKERU!
Dialogue: 0,0:22:39.85,0:22:41.02,ls_ed_eng,,0000,0000,0000,,OH! OH, OH!
Dialogue: 0,0:22:41.02,0:22:43.39,ls_ed_eng,,0000,0000,0000,,HIGH IN THE SKY!
Dialogue: 0,0:22:43.39,0:22:44.31,ls_ed_eng,,0000,0000,0000,,OOH, OOH!
Dialogue: 0,0:22:44.31,0:22:50.65,ls_ed_eng,,0000,0000,0000,,USE SKILL, POWER, AND SOUL!
Dialogue: 0,0:22:50.65,0:22:51.49,ls_ed_eng,,0000,0000,0000,,ZABITAN!
Dialogue: 0,0:22:51.49,0:22:52.24,ls_ed_eng,,0000,0000,0000,,TAKE THAT!
Dialogue: 0,0:22:52.24,0:22:53.03,ls_ed_eng,,0000,0000,0000,,EVIL!
Dialogue: 0,0:22:53.03,0:22:54.24,ls_ed_eng,,0000,0000,0000,,TAKE THAT!
Dialogue: 0,0:22:54.24,0:22:55.41,ls_ed_eng,,0000,0000,0000,,GABURA!
Dialogue: 0,0:22:55.41,0:22:57.41,ls_ed_eng,,0000,0000,0000,,OH! OH, OH, OH, OH!
Dialogue: 0,0:22:57.41,0:23:00.29,ls_ed_eng,,0000,0000,0000,,ROAR, JANKERU!
Dialogue: 0,0:23:00.29,0:23:03.37,ls_ed_eng,,0000,0000,0000,,AKUMAIZER 3!
Dialogue: 0,0:23:03.37,0:23:06.00,ls_ed_eng,,0000,0000,0000,,THERE IS ONLY ONE PATH TO TAKE!
Dialogue: 0,0:23:06.00,0:23:06.75,ls_ed_eng,,0000,0000,0000,,OH!
Dialogue: 0,0:23:06.75,0:23:09.80,ls_ed_eng,,0000,0000,0000,,ONLY ONE PATH TO TAKE!
