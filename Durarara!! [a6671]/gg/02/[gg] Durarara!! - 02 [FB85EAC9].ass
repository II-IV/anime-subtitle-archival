Dialogue: 0,0:00:23.10,0:00:23.94,Names,,0000,0000,0000,,{\pos(1066,194)\frz4.129}Seiji\NYagiri
Dialogue: 0,0:00:26.10,0:00:26.92,Names,,0000,0000,0000,,{\frz7.979\pos(178,536)}Namie\NYagiri
Dialogue: 0,0:00:30.61,0:00:31.42,Names,,0000,0000,0000,,{\pos(854,296)}Shizuo Heiwajima
Dialogue: 0,0:00:33.26,0:00:34.09,Names,,0000,0000,0000,,{\frz12.773\pos(654,306)}Izaya
Dialogue: 0,0:00:33.26,0:00:34.09,Names,,0000,0000,0000,,{\frz12.773\pos(1058,566)}Orihara
Dialogue: 0,0:00:40.08,0:00:40.89,Names,,0000,0000,0000,,{\pos(170,364)\frz18.717}Erika
Dialogue: 0,0:00:40.08,0:00:40.89,Names,,0000,0000,0000,,{\pos(462,294)}Karisawa
Dialogue: 0,0:00:42.27,0:00:43.14,Names,,0000,0000,0000,,{\frz345.435\pos(836,490)}Walker\NYumasaki
Dialogue: 0,0:00:45.14,0:00:45.99,Names,,0000,0000,0000,,{\pos(842,322)}Kyohei
Dialogue: 0,0:00:45.14,0:00:45.99,Names,,0000,0000,0000,,{\frz6.013\pos(870,378)}Kadota
Dialogue: 0,0:00:49.42,0:00:50.23,Names,,0000,0000,0000,,{\frz6.507\pos(1094,220)}Saburo\NTogusa
Dialogue: 0,0:00:58.80,0:00:59.66,Names,,0000,0000,0000,,{\frz355.11\pos(858,78)}Mikado
Dialogue: 0,0:00:58.80,0:00:59.66,Names,,0000,0000,0000,,{\frz352.974\pos(1036,328)}Ryugamine
Dialogue: 0,0:01:02.60,0:01:03.41,Names,,0000,0000,0000,,{\c&HFCFCFC&\pos(554,364)}Masaomi\NKida
